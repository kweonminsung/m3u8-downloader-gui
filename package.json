{
  "main": "./public/electron.js",
  "homepage": "./",
  "name": "m3u8-downloader-gui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mantine/core": "^6.0.4",
    "@mantine/hooks": "^6.0.4",
    "@tabler/icons-react": "^2.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.21",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.3.4",
    "electron-is-dev": "^2.0.0",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "m3u8-parser": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:react": "cross-env BROWSER=none react-scripts start",
    "build:react": "react-scripts build",
    "test:react": "react-scripts test",
    "eject:react": "react-scripts eject",
    "start": "tsc ./public/electron.ts && electron .",
    "start:dev": "tsc ./public/electron.ts && concurrently \"cross-env BROWSER=none npm run start:react\" \"wait-on http://127.0.0.1:3000 && npm run start\"",
    "build": "npm run build:react && electron-builder",
    "release": "npm run build:react && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.2.0",
    "electron-builder": "^23.6.0",
    "wait-on": "^7.0.1"
  }
}
